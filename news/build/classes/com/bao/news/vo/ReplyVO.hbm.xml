<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bao.news.vo">
	<class name="ReplyVO" table="Reply" >
		<id name="id" column="id">
			<generator class="assigned" />
		</id>
		<property name="newsId" column="news_Id" type="string"/>
		<property name="userId" column="user_Id" type="string"/>
		<property name="reply" column="reply" type="string"/>
		<property name="createTime" column="create_time" type="timestamp"/>
	</class>
</hibernate-mapping>
